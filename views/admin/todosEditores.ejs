<%- include('../templates/header.ejs') %>
    <%- include('../templates/navAdmin.ejs') %>


<main>
    <h1><%= titulo %></h1>
    
    <div class="flexContainer">
        <% editores.forEach(element => { %>
            <div>
                <h3 class="textCenter"><%= element.email %></h3>
                <p class="textCenter"><%= element.nombre %></p>
                <p class="textCenter"><%= element.role %></p>
                <p class="textCenter"><%= element._id %></p>
                
                
                
             
                <% if(element.role !="admin") { %>
                    
                    <a href="/adminillo/borrareditor/<%= element._id%>/<%= idUsuario %>/<%= nombreUsuario %>" class="btnTabla">Eliminar</a>
                    <% } %>

            </div>
        <% }) %>
    </div>
</main>
<%- include('../templates/footer.ejs') %>