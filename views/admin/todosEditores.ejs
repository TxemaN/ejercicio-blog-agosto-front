<%- include('../templates/header.ejs') %>


<main>
    <h1><%= titulo %></h1>
    
    <div class="flexContainer">
        <% editores.forEach(element => { %>
            <div>
                <h3 class="textCenter"><%= element.email %></h3>
                <p class="textCenter"><%= element.nombre %></p>
                <p class="textCenter"><%= element._id %></p>
                
                
                
                <a href="editar/<%= element.email %>" class="btnTabla">Editar</a>
                <a href="borrareditor/<%= element._id %>" class="btnTabla">Borrar</a>
                
            </div>
        <% }) %>
    </div>
</main>