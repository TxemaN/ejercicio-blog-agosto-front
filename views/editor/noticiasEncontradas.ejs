<%- include('../templates/header.ejs') %>
    <%- include('../templates/navLogeado.ejs') %>


        <main>


            <h1>
                <%= titulo %>
            </h1>


            <div class="flexContainer">
                <% noticias.forEach(element=> { %>
                    <article>
                        <h3><a href="/editor/buscadaAjena/<%= element.titulo %>/<%= idUsuario %>/<%= nombreUsuario %>" class="btnTabla">
                                <%= element.titulo %>
                            </a></h3>
                        <p class="parrafoCorto">
                            <%= element.noticia %>
                        </p>
                        <div class="fotos">
                            <figure>

                                <img src="<%= element.imagen%>" alt="">
                            </figure>
                        </div>



                    </article>
                    <% }) %>
            </div>
        </main>