<%- include('../templates/header.ejs') %>
    <%- include('../templates/navLogeado.ejs') %>


        <main>


            <h1>
                <%= titulo %>
            </h1>



            <% noticias.forEach(element=> { %>
                <article class="noticia">
                    <h3><a href="/editor/buscadaAjena/<%= element.titulo %>/<%= idUsuario %>/<%= nombreUsuario %>"
                            class="btnTabla">
                            <%= element.titulo %>
                        </a></h3>
                    <p>CREADA POR: <a
                            href="/editor/creadapor/<%= element.creador %>/<%= element.nombrecreador %>/<%= idUsuario %>/<%= nombreUsuario %>"
                            class="btnTabla">
                            <%= element.nombrecreador %>
                        </a> el <%= element.fecha %>
                    </p>
                    <div class="parrafoCorto">
                        <p>
                            <%= element.intronoticia %>
                        </p>
                        <div>


                            <img src="/<%= element.imagensrc%>" alt="<%= element.imagenalt%>">

                        </div>
                    </div>

                </article>
                <% }) %>

        </main>
        <%- include('../templates/footer.ejs') %>