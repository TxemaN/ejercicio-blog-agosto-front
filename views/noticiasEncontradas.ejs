<%- include('templates/header.ejs') %>
<%- include('templates/nav.ejs') -%>


<main>
    
    
    <h1><%= titulo %></h1>
   
    <div class="flexContainer">
        <% noticias.forEach(element => { %>
            <article>
                <h3><a href="buscada/<%= element.titulo %>" class="btnTabla"><%= element.titulo %></a></h3>
                <p class="parrafoCorto"><%= element.noticia %></p>
                <div>
                    <figure>
                       
                        <img src="<%= element.imagen%>" alt="">
                    </figure>
                </div>
               
                
              
            </article>
        <% }) %>
    </div>
</main>